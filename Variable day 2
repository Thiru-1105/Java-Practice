class Variables {
    public static void main(String[] args) {

        int age = 21;
        double weight = 70.5;
        char grade = 'A';
        String name = "Thirumurugan";

        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
        System.out.println("Weight: " + weight);
        System.out.println("Grade: " + grade);

    }
}
